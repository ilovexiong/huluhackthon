{
  "name": "Hooli",
  "version": "0.1",
  "description": "Hulu Video Chat Room & player rolling down zooming effect.",
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "content_scripts": [ {
    "css": ["bootstrap.min.css"],
    "js": [ "jquery-3.1.0.min.js", "bootstrap.min.js", "d3.min.js", "hooliContent.js", "hooliChatRoom.js" ],
    "matches": [ "http://www.hulu.com/watch/*"]
  }],
  "icons": {"128": "icon.png"},
  "browser_action": {
      "default_title": "Hooli",
      "default_icon": "icon.png"
  },
  "manifest_version": 2
}
